
input DictSpecInput {
    #allScalars(DictSpec)
    id?
}

specification DictSpecSpec {
    #allScalars
    like/i(getId)
    ge(getMaxCreatedTime)
    le(getMaxCreatedTime)
    ge(getMaxEditedTime)
    le(getMaxEditedTime)
    ge(getMinEditedTime)
    le(getMinEditedTime)
    ge(getMinCreatedTime)
    le(getMinCreatedTime)
    like/i(getDictEnName)
    like/i(getKeyEnName)
    like/i(getDictName)
    like/i(getKeyName)
}